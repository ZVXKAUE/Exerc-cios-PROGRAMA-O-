#include <iostream>
using namespace std;

int main() {
    double celsius, fahrenheit;


    cout << "Digite a temperatura em graus Celsius: ";
    cin >> celsius;

    
    fahrenheit = (9.0 / 5.0) * celsius + 32;

    
    cout << "Temperatura em Fahrenheit: " << fahrenheit << "Â°F" << endl;

    return 0;
}
