#include <iostream>
using namespace std;

int main() {
    double a, b;

    cout << "Digite o primeiro número: ";
    cin >> a;

    cout << "Digite o segundo número: ";
    cin >> b;

    double quadradoDaDiferenca = (a - b) * (a - b);

    double diferencaDosQuadrados = (a * a) - (b * b);

    cout << "O quadrado da diferença entre os dois números é: " << quadradoDaDiferenca << endl;
    cout << "A diferença dos quadrados entre os dois números é: " << diferencaDosQuadrados << endl;

    return 0;
}
